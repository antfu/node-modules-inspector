{
  "type": "module",
  "version": "0.0.2",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "nuxt dev packages/node-modules-inspector/src",
    "start": "pnpm -C packages/node-modules-inspector run start",
    "prepare": "nuxt prepare packages/node-modules-inspector/src && npx simple-git-hooks",
    "lint": "nuxi prepare && eslint .",
    "test": "vitest",
    "release": "bumpp -r && pnpm publish -r",
    "typecheck": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@iconify-json/carbon": "catalog:",
    "@iconify-json/catppuccin": "catalog:",
    "@iconify-json/logos": "catalog:",
    "@iconify-json/ph": "catalog:",
    "@iconify-json/ri": "catalog:",
    "@iconify-json/simple-icons": "catalog:",
    "@nuxt/eslint": "catalog:",
    "@shikijs/langs-precompiled": "catalog:",
    "@shikijs/transformers": "catalog:",
    "@types/connect": "catalog:",
    "@types/d3": "catalog:",
    "@types/d3-hierarchy": "catalog:",
    "@types/ws": "catalog:",
    "@typescript-eslint/utils": "catalog:",
    "@unocss/eslint-config": "catalog:",
    "eslint": "catalog:",
    "lint-staged": "catalog:",
    "nuxt": "catalog:",
    "nuxt-eslint-auto-explicit-import": "catalog:",
    "simple-git-hooks": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vitest": "catalog:",
    "vue-tsc": "catalog:"
  },
  "pnpm": {
    "overrides": {
      "nitropack": "catalog:"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
