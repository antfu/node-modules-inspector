{
  "name": "node-modules-inspector",
  "type": "module",
  "version": "0.3.0",
  "description": "A Node Modules Inspector",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/node-modules-inspector#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/node-modules-inspector.git"
  },
  "bugs": "https://github.com/antfu/node-modules-inspector/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": "bin.mjs",
  "files": [
    "bin.mjs",
    "dist"
  ],
  "scripts": {
    "dev": "pnpm run -r stub && cd src && nuxi dev",
    "stub": "unbuild --stub",
    "build": "pnpm run wc:prepare && nuxi build src && unbuild",
    "build:debug": "NUXT_DEBUG_BUILD=true pnpm run build",
    "start": "node ./bin.mjs",
    "prepack": "pnpm build",
    "dev:prepare": "nuxi prepare src",
    "wc:prepare": "rollup -c",
    "wc:build": "pnpm run wc:prepare && NMI_BACKEND=webcontainer nuxi build src && unbuild",
    "wc:dev": "pnpm run -r stub && pnpm run wc:prepare && cd src && NMI_BACKEND=webcontainer nuxi dev"
  },
  "dependencies": {
    "ansis": "catalog:deps",
    "birpc": "catalog:deps",
    "cac": "catalog:deps",
    "fast-npm-meta": "catalog:deps",
    "get-port-please": "catalog:deps",
    "h3": "catalog:deps",
    "launch-editor": "catalog:deps",
    "mlly": "catalog:testing",
    "mrmime": "catalog:deps",
    "node-modules-tools": "workspace:*",
    "ohash": "catalog:deps",
    "open": "catalog:deps",
    "p-limit": "catalog:deps",
    "pathe": "catalog:deps",
    "publint": "catalog:deps",
    "structured-clone-es": "catalog:deps",
    "tinyglobby": "catalog:deps",
    "unconfig": "catalog:deps",
    "unstorage": "catalog:deps",
    "ws": "catalog:deps"
  },
  "devDependencies": {
    "@types/semver": "catalog:types",
    "@unocss/nuxt": "catalog:build",
    "@vueuse/nuxt": "catalog:build",
    "@webcontainer/api": "catalog:fontend",
    "@xterm/addon-fit": "catalog:fontend",
    "@xterm/xterm": "catalog:fontend",
    "d3": "catalog:fontend",
    "d3-hierarchy": "catalog:fontend",
    "d3-shape": "catalog:fontend",
    "floating-vue": "catalog:fontend",
    "fuse.js": "catalog:fontend",
    "idb-keyval": "catalog:fontend",
    "modern-screenshot": "catalog:fontend",
    "semver": "catalog:deps",
    "theme-vitesse": "catalog:fontend",
    "vite-hot-client": "catalog:fontend"
  }
}
